{
    "name": "My Fourth Extension", 
    "version": "0.1",
    "manifest_version": 2,
    "description": "背景色を変える with option page",
    "permissions": [
        "tabs", "http://*/*"
    ],

    "options_page": "options.html",

    "background": {
        "scripts": ["background.js"]
    },

    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "My Fourth Extension"
    }
}
